{
    "name": "firephp",
    "engine": [
        "node"
    ],
    "main": "lib/project.js",
    "scripts": {
        "import": {
            "location": "./",
            "module": "/scripts/import.js"
        },
        "build": {
            "location": "./",
            "module": "/scripts/build.js"
        },
        "test": {
            "location": "./",
            "module": "/scripts/test.js"
        },
        "docs": {
            "locator": {
                "id": "github.com/pinf/docs-js/"
            },
            "options": {
                "packagePath": "../"
            }
        }
    },
    "dependencies": [
        {
            "id": "github.com/firephp/firephp/"
        }
    ],
    "mappings": {
        "nodejs": {
            "id": "nodejs.org/"
        },
        "pinf": {
            "id": "pinf.org/loader/"
        },
        "modules": {
            "id": "github.com/pinf/modules-js/"
        }
    }
}