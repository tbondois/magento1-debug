                "locator": {
                    "location": "./build/lib.phar"
                },
                "scope": "lib",
                "label": "PHAR Archive",
                "contentType": "application/zip",
                "suffix": ".phar"
            }
        },
        "github.com/pinf/docs-js/-meta/strawman/config/0.1": {
            "include": "./docs/docs.json"
        }
    }
